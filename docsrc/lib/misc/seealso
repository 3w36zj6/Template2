[% RETURN UNLESS seealso -%]
=head1 SEE ALSO

[%  FOREACH item = seealso.split(',\s*');
	item = item.split('\|');
	name = item.0;
	IF item.0.search('^CPAN::');
	    name = name.replace('^CPAN::', '');
	    docset.cpan.$name = name;
	END;
#	(link = item.1) ? "L<$name|$link>" : "L<$name>";
	link = item.1 or name;
	"L<$name|$link>";
        ', ' UNLESS loop.last;
    END
-%]
